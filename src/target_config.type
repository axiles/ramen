// Describe the operations that ought to be running in the stream processor.
// This is a list of individual entries, each describing a program.
($program_name;{
  enabled: bool;
  debug: bool;
  report_period: float;
  cwd: $file_path;  // If not empty
  params: $program_run_parameter[];
  on_site: string;  // Globs
  automatic: bool;
})[]
