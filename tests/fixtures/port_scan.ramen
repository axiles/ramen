-- vim: ft=sql expandtab

DEFINE csv AS
  READ FILE "port_scan.csv" (
    source string,
    first float,
    last float,
    seqnum u32,
    engine_type u8,
    engine_id u8,
    sampling_type u8,
    sampling_rate u16,
    src ip4,
    dst ip4,
    next_hop ip4,
    src_port u16,
    dst_port u16,
    in_iface u16,
    out_iface u16,
    packets u32,
    bytes u32,
    tcp_flags u8,
    ip_proto u8,
    ip_tos u8,
    src_as u16,
    dst_as u16,
    src_mask u8,
    dst_mask u8)
  EVENT STARTS AT first AND STOPS AT last;
