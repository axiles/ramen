-- vim: ft=sql expandtab

DEFINE f AS
  FROM fixtures/n123/csv
  SELECT SUM n
  COMMIT BEFORE sum_n > 4;

-- Note: we won't have "Volvo" at the end since it will never be commited:
DEFINE count_cars AS
  FROM fixtures/cars/csv
  SELECT
    manufacturer,
    lag locally manufacturer AS prev_manufacturer,
    SUM 1 AS num_models
  COMMIT BEFORE out.manufacturer != out.prev_manufacturer;
